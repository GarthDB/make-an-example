{
  "name": "make-an-example",
  "version": "0.0.0-semantically-released",
  "description": "Helper method to help make examples for documentation.",
  "main": "lib/index.js",
  "scripts": {
    "start": "package-scripts",
    "test": "package-scripts test",
    "prepublish": "package-scripts build"
  },
